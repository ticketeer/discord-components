{
  "name": "@discord-components/vue",
  "version": "0.2.1",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/vue.umd.js",
  "module": "./dist/vue.mjs",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue.mjs",
      "require": "./dist/vue.umd.js"
    },
    "./dist/*": "./dist/*",
    "./styles": "./dist/style.scss"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types",
    "build:types": "vue-tsc --project tsconfig.build.json --declaration --emitDeclarationOnly --outDir dist/types ",
    "serve": "vite preview",
    "lint": "eslint . --ext js,ts,tsx,vue",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@discord-components/core": "^0.2.1"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/vue3": "^6.5.15",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/eslint-config-typescript": "^11.0.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.2",
    "eslint-plugin-vue": "^9.9.0",
    "prettier": "^2.8.3",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vue": "^3.2.45",
    "vue-loader": "^17.0.1",
    "vue-tsc": "^1.0.24"
  },
  "gitHead": "1e5ae8e0fe8afa656bfed9b95c9b61fd7aad3d4d"
}
